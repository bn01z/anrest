{
  "name": "anrest",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "build-api": "ng build @anrest/api",
    "build-cache": "ng build @anrest/cache",
    "build-auth": "ng build @anrest/auth",
    "build-loader": "ng build @anrest/loader",
    "build-all": "npm run build-api && npm run build-cache && npm run build-auth && npm run build-loader",
    "pack-api": "npm run build-api && cp ./LICENSE dist/anrest/api && cd dist/anrest/api && npm pack",
    "pack-cache": "npm run build-cache && cp ./LICENSE dist/anrest/cache && cd dist/anrest/cache && npm pack",
    "pack-auth": "npm run build-auth && cp ./LICENSE dist/anrest/auth && cd dist/anrest/auth && npm pack",
    "pack-loader": "npm run build-loader && cp ./LICENSE dist/anrest/loader && cd dist/anrest/loader && npm pack",
    "pack-all": "npm run pack-api && npm run pack-cache && npm run pack-auth && npm run pack-loader"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "ngx-cookie-service": "^2.1.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular-devkit/build-ng-packagr": "~0.12.0",
    "@angular/cli": "~7.2.3",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
